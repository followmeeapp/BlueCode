{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "declaration": true,
        "sourceMap": true,
        "outDir": "bin",
        "rootDir": "src",
        "watch": true
    },
    "exclude": [
      ".vscode",
      "bin",
      "lmdb",
      "node_modules",
      "src/defs",
      "src/server/defs",
      "src/server/ignored"
    ]
}
